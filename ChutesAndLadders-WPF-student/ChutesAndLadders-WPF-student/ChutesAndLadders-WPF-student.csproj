    <RootNamespace>ChutesAndLadders_WPF</RootNamespace>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="ChutesAndLadders.png" />
  </ItemGroup>

